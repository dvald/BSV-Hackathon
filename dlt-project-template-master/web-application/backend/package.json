    "@types/cookie-parser": "1.4.4",
    "@types/express": "4.17.17",
    "@types/express-fileupload": "1.4.1",
    "@types/express-useragent": "1.0.2",
    "@types/express-ws": "3.0.1",
    "@types/ffprobe": "1.1.8",
    "@types/i18n": "0.13.6",
    "@types/ioredis": "4.28.10",
