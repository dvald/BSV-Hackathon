{
  "name": "backend",
  "version": "0.0.1",
  "description": "Backend Server",
  "main": "index.js",
  "scripts": {
    "watch": "npm run watch:ts",
    "watch:ts": "npm run build:ts -- --watch --diagnostics",
    "prebuild": "npm run lint",
    "compile": "tsc --pretty",
    "build": "npm run server-build",
    "server-build": "rimraf dist && tsc --pretty",
    "lint": "eslint --fix src",
    "setup-mongo": "node dist/setup-mongo.js",
    "update-locales": "npm run update-translations",
    "update-translations": "node scripts/translate-file-maker.js",
    "update-db-scripts": "node scripts/gen-db-scripts.js",
    "reset-blockchain-sync": "node dist/reset-blockchain-sync.js",
    "update-api-bindings": "node scripts/gen-api-bindings.js",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"ts-node src/index.ts\"",
    "start": "node dist/index.js",
    "unit-tests": "mocha -r ts-node/register --config test/mocha/config.function.json",
    "api-tests": "mocha -r ts-node/register --config test/mocha/config.api.json",
    "test": "npm run unit-tests && npm run api-tests"
  },
  "dependencies": {
    "@asanrom/async-tools": "1.1.1",
    "@asanrom/express-swagger-generator": "2.2.0",
    "@asanrom/javascript-object-sanitizer": "1.1.0",
    "@asanrom/smart-contract-wrapper": "5.4.4",
    "@bsv/auth-express-middleware": "^1.2.3",
    "@bsv/payment-express-middleware": "^1.2.3",
    "@bsv/sdk": "^1.9.11",
    "@bsv/wallet-toolbox": "^1.7.13",
    "backend": "file:",
    "compression": "1.8.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "decimal.js": "10.4.3",
    "did-jwt": "^8.0.18",
    "did-jwt-vc": "^4.0.16",
    "did-resolver": "^4.1.0",
    "dotenv": "16.3.1",
    "ethereum-cryptography": "2.1.2",
    "ethers": "6.14.4",
    "ethr-did-resolver": "^11.0.5",
    "express": "4.21.2",
    "express-async-errors": "3.1.1",
    "express-fileupload": "1.4.0",
    "express-useragent": "1.0.15",
    "ffprobe": "1.1.2",
    "form-data": "4.0.4",
    "i18n": "0.15.1",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.2",
    "lru-cache": "10.0.1",
    "md5-file": "5.0.0",
    "mime-types": "2.1.35",
    "nodemailer": "7.0.10",
    "rimraf": "5.0.1",
    "speakeasy": "2.0.0",
    "stripe": "^18.2.0",
    "swagger2-to-postmanv2": "^2.1.0",
    "tsbean-driver-mongo": "4.0.2",
    "tsbean-driver-mysql": "3.0.0",
    "tsbean-driver-postgres": "3.0.1",
    "tsbean-orm": "3.0.0",
    "uuid": "11.1.0",
    "winston": "3.11.0",
    "winston-elasticsearch": "0.19.0",
    "ws": "8.18.0"
  },
  "devDependencies": {
    "@asanrom/request-axios": "1.0.6",
