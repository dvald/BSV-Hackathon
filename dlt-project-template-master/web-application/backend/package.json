    "@types/nodemailer": "6.4.10",
    "@types/rimraf": "3.0.2",
    "@types/speakeasy": "2.0.7",
    "@types/ws": "8.5.5",
    "chai": "4.3.8",
    "eslint": "9.31.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "ignore": "5.2.4",
    "mocha": "11.5.0",
    "nodemon": "3.0.2",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "typescript-eslint": "8.26.1"
  },
